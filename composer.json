{
    "name": "sagor/github-updater",
    "description": "A Laravel package for automated Github pulls and Artisan commands by moh-sagor.",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/process": "~3.4 || ^4.4 || ^5.0 || ^6.0 || ^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.0 || ^10.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sagor\\GithubUpdater\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sagor\\GithubUpdater\\Providers\\GithubUpdaterServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Md Sagor Hossain",
            "email": "sagorhassain4@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "keywords": ["laravel", "github", "updater", "git", "artisan"],
    "homepage": "https://github.com/moh-sagor/github-updater",
    "repository": {
        "type": "git",
        "url": "https://github.com/moh-sagor/github-updater.git"
    },
    "autoload-dev": {
        "psr-4": {
            "Sagor\\GithubUpdater\\Tests\\": "tests/"
        }
    }
}
